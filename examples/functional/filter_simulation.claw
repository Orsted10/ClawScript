// Simulating filter/map behavior
fn processNumbers(f) {
    // Process numbers 1-5 with function f
    print f(1);
    print f(2);
    print f(3);
    print f(4);
    print f(5);
}

fn isEven(n) {
    return n % 2 == 0;
}

fn square(n) {
    return n * n;
}

fn triple(n) {
    return n * 3;
}

print "Squaring numbers 1-5:";
processNumbers(square);

print "Tripling numbers 1-5:";
processNumbers(triple);
